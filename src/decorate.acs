Actor CoolLauncher : RocketLauncher replaces RocketLauncher 1337
{
//$Category "DNIWE"
States
{
Ready:
    MISG A 1 Offset(0, 32)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(32, 32)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(-32, 32)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(0, 64)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(32, 64)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(-32, 64)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(64, 32)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(-64, 32)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(64, 64)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(-64, 64)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(32, 32)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(-32, 32)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(64, 96)
	MISG A 0 A_WeaponReady
    MISG A 1 Offset(-64, 96)
	MISG A 0 A_WeaponReady
	loop
Fire:
    MISG C 4
	MISG D 4 A_GunFlash
	MISG E 4 A_FireCustomMissile("CoolRocket0")
    MISG B 4 A_FireCustomMissile("CoolRocket1")
    MISG B 4
    goto Super::Fire+2
}
}

